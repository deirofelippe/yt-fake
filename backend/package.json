{
  "name": "yt-fake-backend",
  "version": "1.0.0",
  "description": "Backend da plataforma inspirado no youtube com funcionalidades a mais.",
  "main": "src/main/index.ts",
  "engines": {
    "node": "16.x"
  },
  "author": "Felippe Deir√≥ <sergiofelippe.deiro@gmail.com>",
  "scripts": {
    "test": "cross-env NODE_ENV=test npx jest --config jest.config.js --runInBand --detectOpenHandles",
    "test:ci": "npm run test -- --silent --verbose",
    "test:dev": "cross-env NODE_ENV=development npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage --silent --verbose",
    "test:mutation": "cross-env NODE_ENV=test npx stryker run",
    "test:watch": "npm run test -- --watch",
    "prebuild": "npx rimraf ./dist",
    "build": "npx swc ./src -d ./dist",
    "heroku-cleanup": "rm -rf ./src ./dist/test",
    "lint": "npx eslint --max-warnings=0 . --ext .ts",
    "check": "npx tsc --checkJ",
    "start": "node ./dist/main/index.js",
    "ngrok": "npx ts-node --transpileOnly --esm ./ngrok.ts",
    "dev": "npx ts-node-dev --respawn --transpile-only ./server.ts"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "mercadopago": "^1.5.14",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@stryker-mutator/core": "^6.1.2",
    "@stryker-mutator/jest-runner": "^6.1.2",
    "@stryker-mutator/typescript-checker": "^6.1.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.189",
    "@swc/jest": "^0.2.21",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/joi": "^17.2.3",
    "@types/mercadopago": "^1.5.6",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "jest": "^27.5.1",
    "ngrok": "^4.3.1",
    "nock": "^13.2.9",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "smid": "^0.1.1",
    "sqlite3": "5.0.0",
    "supertest": "6.1.6",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deirofelippe/yt-fake"
  },
  "keywords": [
    "node",
    "typescript",
    "heroku",
    "express",
    "typeorm",
    "docker",
    "postgres",
    "github-actions",
    "sqlite",
    "jest"
  ],
  "license": "MIT"
}
